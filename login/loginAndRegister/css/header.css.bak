@media screen and (min-width:1400px){	  
	body,html{
		  width:100%;
	 }
		
}

html,body{
	width:1370px;
	/*height:auto;*/
    /* height:3055px; */
    font-size:16px;
    color:rgba(102,102,102,1);
    overflow:scroll;
	/*  */
	 margin:0 auto;
	 overflow-x:hidden;
}
body{
	overflow-y:hidden;
}
header{
    width:100%;
}
#nav{
    width:100%;
    height:57px;
    background:rgba(255,255,255,1);
    overflow:hidden;
}
#nav .nav_ul{
    display:block;
    width:62.6%;
    height:39px;
    margin:0 auto;
    margin-top:9px;
}
#nav .nav_ul li{
    float:left;
    text-align: center;
    height:35px;
    margin-top:8.5px;
    font-size:0.94rem;
    font-family:PingFang-SC-Bold;
    color:rgba(36,199,139,1);
}
#nav ul .active_a{
    color:rgba(36,199,139,1);
}
#nav ul li:nth-of-type(1){
    width:18.9%;
    margin-top:0;
    margin-right:3.96%;
}
#nav ul li:nth-of-type(1) img{
    width:163px;
    height:39px;
}


#nav ul li:nth-of-type(2){
    display:block;
    width:3.96%;
    margin-right:3.96%

}
#nav ul li:nth-of-type(3){
    display:block;
    width:7.93%;
    margin-right:3.96%;
}
#nav ul li:nth-of-type(4){
    display:block;
    width:5.95%;
    margin-right:36.4%;
}

#nav ul li:nth-of-type(2):hover,#nav ul li:nth-of-type(3):hover,#nav ul li:nth-of-type(4):hover{
    border-bottom:2px solid #24C78B;
}
#nav ul li:nth-of-type(5){
        width:6.7%;
        margin-right:2%;
}
